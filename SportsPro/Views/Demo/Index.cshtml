@model IEnumerable<SimpleMvcApp.Models.Person>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>People List</title>
</head>
<body>
    <h1>People</h1>
    <ul>
    @foreach(var person in Model)
    {
        <li>@person.Name (ID: @person.Id)</li>
    }
    </ul>
</body>
</html>
